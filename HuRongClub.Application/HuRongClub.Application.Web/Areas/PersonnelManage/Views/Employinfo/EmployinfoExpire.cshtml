@{;
ViewBag.Title = "合同续费表单页面";
Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = request('keyValue');
    $(function () {
        initControl();
    });
    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var $firedate = $.trim($("#firedate").val());
        $.SaveForm({
            url: "../../PersonnelManage/Employinfo/ExpireFrom?keyValue=" + keyValue,
            param: { firedate: $firedate },
            loading: "正在保存数据...",
            success: function (data) {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        })
    }
</script>
<div style="margin-top: 50px; margin-right: 30px;">
    <table class="form">
        <tr>
            <td class="formTitle">合同到期日期<font face="宋体">*</font></td>
            <td class="formValue">
                <input id="firedate" type="text" class="form-control input-datepicker" isvalid="yes" checkexpession="NotNull" onfocus="WdatePicker()" readonly="readonly" />
            </td>
        </tr>
    </table>
</div>