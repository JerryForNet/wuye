@{;
ViewBag.Title = "离职表单页面";
Layout = "~/Views/Shared/_Form.cshtml";
}

<script>
    var keyValue = request('keyValue');
    $(function () {
        initControl();
    });
    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var $fireoutdate=$.trim($("#fireoutdate").val());
        $.SaveForm({
            url: "../../PersonnelManage/Employinfo/Dimission?keyValue=" + keyValue,
            param: { fireoutdate: $fireoutdate },
            loading: "正在保存数据...",
            success: function (data) {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        })
    }
</script>
<div style="margin-top: 50px; margin-right: 30px;">
    <table class="form">
        <tr>
            <td class="formTitle">离职日期<font face="宋体">*</font></td>
            <td class="formValue">
                <input id="fireoutdate" type="text" class="form-control input-datepicker" isvalid="yes" checkexpession="NotNull" onfocus="WdatePicker()" readonly="readonly" />
            </td>
            <td class="formTitle"></td>
            <td class="formValue"> </td>
        </tr>
    </table>
</div>